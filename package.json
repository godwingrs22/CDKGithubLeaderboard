{
  "name": "cdk-github-leaderboard",
  "version": "1.0.0",
  "description": "Track and analyze external community contributions to AWS CDK",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "deploy": "npm run build && cdk deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/godwingrs22/CDKGithubLeaderboard.git"
  },
  "keywords": [
    "aws-cdk",
    "github",
    "community",
    "analytics"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godwingrs22/CDKGithubLeaderboard/issues"
  },
  "homepage": "https://github.com/godwingrs22/CDKGithubLeaderboard#readme",
  "dependencies": {
    "@octokit/graphql": "^5.0.0",
    "aws-cdk-lib": "2.178.1",
    "constructs": "^10.0.0",
    "dotenv": "^16.0.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.0",
    "typescript": "^4.9.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "prettier": "^2.0.0"
  }
}
